							<xs:annotation>
								<xs:documentation xml:lang="es">Fecha y hora incio de la descarga de los desechos</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="RMDeliveryEnd" type="xs:dateTime">
							<xs:annotation>
								<xs:documentation xml:lang="es">Fecha y hora del fin la descarga de los desechos</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="RMResidueData" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="es">Datos del Residuo transportado. Identifica el residuo, proporcionado todos los parámetros que la legislación requiere para realizar traslados de residuos MAROL. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="RMResidueIdentification" type="wassup:marpolResidueType">
							<xs:annotation>
								<xs:documentation xml:lang="es">Identificación del residuo</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="RMMovementData">
							<xs:annotation>
								<xs:documentation xml:lang="es">Datos del traslado</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="RMMovementVolume" type="common:decimalUnitsType">
										<xs:annotation>
											<xs:documentation xml:lang="es">Volumen Total entregado en m3</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="RMMovementObservations" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="es">Observaciones generales</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="RMCode" type="wassup:RMCodeType" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="es">Código de Recibo de Residuos MARPOL</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RMAnnulledDocument" type="wassup:annulledRMCodeType">
			<xs:annotation>
				<xs:documentation xml:lang="es">Documento anulado. En el tipo annulledDocumentCodeType se especifica cómo se informa este campo.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RMYear" type="xs:gYear" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="es">Año</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RMDate" type="xs:date">
			<xs:annotation>
				<xs:documentation xml:lang="es">Fecha de creación</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="wasteMAGType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Memoria anual del gestor. Anualmente el gestor de residuos tóxicos y peligrosos deberá presentar una memoria anual de actividades ante el órgano competente de la comunidad autónoma.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MAGRepresentationData" type="common:representationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="es">Datos del representante</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MAGOperator" type="wassup:MAGOperatorType"/>
			<xs:element name="MAGInstallation" type="wassup:wasteMAGCenterType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Datos de la instalación</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MAGCenterHomologation" type="wassup:managerHomologationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="es">Parámetros de homologación</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MAGCenterProcesses" type="wassup:MAGTreatmentProcessesType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="es">Datos de cada uno de los procesos de gestión de residuos (peligrosos y no peligrosos).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MAGMaintenanceProcesses" type="wassup:MAGMaintenanceProcessType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="es">Datos de cada uno de los procesos de servicios generales, limpieza de instalaciones y mantenimiento que generan residuos (peligrosos y no peligrosos).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="MAGCode" type="wassup:MAGCodeType" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="es">Número de memoria</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MAGAnnulledDocument" type="wassup:annulledMAGCodeType">
			<xs:annotation>
				<xs:documentation xml:lang="es">Documento anulado. En el tipo annulledDocumentCodeType se especifica cómo se informa este campo.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MAGYear" type="xs:gYear" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="es">Año de declaración</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MAGDate" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="es">Fecha de declaración</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MAGState" type="common:declarationStateType" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="es">Estado de la memoria resumen</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MAGCA" type="common:CAType" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="es">Código Comunidad Autónoma</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MAGCADescription" type="xs:string">
			<xs:annotation>
				<xs:documentation xml:lang="es">Descripción Comunidad Autónoma</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="wasteSTType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Contiene todos los campos necesarios para el flujo de Estadísticas de Residuos. Está basado en el Reglamento (CE) 782/2005.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="STRepresentationData" type="common:representationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="es">Datos del representante</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="STGENER" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="es">Conjunto 1: Generación de residuos GENER</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="wassup:statisticsSetType">
							<xs:sequence>
								<xs:element name="STGENERActivityNumber" type="wassup:statisticActivityType">
									<xs:annotation>
										<xs:documentation xml:lang="es">Número de Actividad. Código referido a la NACE (véase lista C del reglamento (CE) 782/2005)</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="STINCIN" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="es">Conjunto 2: Incineración INCIN</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="wassup:statistics2SetType">
							<xs:sequence>
								<xs:element name="STOriginCountry" type="common:countryDataType">
									<xs:annotation>
										<xs:documentation xml:lang="es">Identificación del País Origen</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="STOriginCA" type="common:CADataType">
									<xs:annotation>
										<xs:documentation xml:lang="es">Identificación de la Comunidad Autónoma Origen</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="STDestinationCountry" type="common:countryDataType">
									<xs:annotation>
										<xs:documentation xml:lang="es">Identificación del País Destino</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="STRECOV" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="es">Conjunto 3: Actividades que dan lugar a recuperación RECOV</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="wassup:statistics2SetType">
							<xs:sequence>
								<xs:element name="STOriginCountry" type="common:countryDataType">
									<xs:annotation>
										<xs:documentation xml:lang="es">Identificación del País Origen</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="STOriginCA" type="common:CADataType">
									<xs:annotation>
										<xs:documentation xml:lang="es">Identificación de la Comunidad Autónoma Origen</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="STDestinationCountry" type="common:countryDataType">
									<xs:annotation>
										<xs:documentation xml:lang="es">Identificación del País Destino</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="STDISPO" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="es">Conjunto 4: Eliminación DISPO</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="wassup:statistics2SetType">
							<xs:sequence>
								<xs:element name="STOriginCountry" type="common:countryDataType">
									<xs:annotation>
										<xs:documentation xml:lang="es">Identificación del País Origen</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="STOriginCA" type="common:CADataType">
									<xs:annotation>
										<xs:documentation xml:lang="es">Identificación de la Comunidad Autónoma Origen</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="STDestinationCountry" type="common:countryDataType">
									<xs:annotation>
										<xs:documentation xml:lang="es">Identificación del País Destino</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="STREGIO" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="es">Conjunto 5: Número y capacidad de instalaciones de recuperación y eliminación; cobertura del sistema de recogida de residuos por región REGIO</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="STREGIOActivityNumber">
							<xs:annotation>
								<xs:documentation xml:lang="es">Actividad de recuperación o eliminación</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="wassup:activityType">
									<xs:enumeration value="1"/>
									<xs:enumeration value="2"/>
									<xs:enumeration value="3"/>
									<xs:enumeration value="4"/>
									<xs:enumeration value="5"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="STOperation" type="wassup:table2Type">
							<xs:annotation>
								<xs:documentation xml:lang="es">Código de operación recuperación o eliminación</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="STREGIOProvince" type="common:provinceType">
							<xs:annotation>
								<xs:documentation xml:lang="es">Código de la Provincia</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="STREGIOVariable" type="wassup:statisticCapacityVariableType">
							<xs:annotation>
								<xs:documentation xml:lang="es">Variable</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="STREGIOMeasure" type="wassup:statisticCapacityVariableType">
							<xs:annotation>
								<xs:documentation xml:lang="es">Medida</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="STREGIOValue">
							<xs:annotation>
								<xs:documentation xml:lang="es">Valor</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="12"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="STREGIOUpdateIndicator" type="wassup:statisticUpdateType">
							<xs:annotation>
								<xs:documentation xml:lang="es">Indicador de actualización</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="STREGIOConfidentialityIndicator" type="wassup:statisticConfidentialityType">
							<xs:annotation>
								<xs:documentation xml:lang="es">Indicador de confidencialidad</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="STCode" type="wassup:STCodeType" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="es">Número de ST</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="STAnnulledDocument" type="wassup:annulledSTCodeType">
			<xs:annotation>
				<xs:documentation xml:lang="es">Documento anulado. En el tipo annulledDocumentCodeType se especifica cómo se informa este campo.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="STYear" type="xs:gYear" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="es">Año</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="STCA" type="common:CAType" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="es">Código Comunidad Autónoma</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="STCADescription" type="xs:string">
			<xs:annotation>
				<xs:documentation xml:lang="es">Descripción Comunidad Autónoma</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="wasteDocumentType">
		<xs:choice>
			<xs:element name="wasteSA" type="wasteSAType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Solicitud de admisión. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wasteDA" type="wasteDAType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Documento de Aceptación. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wasteNT" type="wasteNTType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Notificación de traslado. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wasteDCS" type="wasteDCSType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Documento de control y seguimiento. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wasteWSN" type="wasteWSNType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Documento de notificación de movimiento transfronterizos/ traslados de residuos (anexo IA) - Waste Shipment Notification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wasteWSM" type="wasteWSMType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Documento de movimiento para movimientos transfronterizos/ traslados de residuos (anexo IB) - Waste Shipment Moviment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wasteGWS" type="wasteGWSType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Información que debe acompañar a los traslados de residuos que se refiere el artículo 3, apartado 2 y 4 (Anexo VII del RD 1084/2009) - Green Waste Shipment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wasteNM" type="wasteNMType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Notificación MARPOL. 	Notificación antes de entrar en el puerto de destino (Anexo II del RD 1084/2009) o notificación reducida para embarcaciones de pesca fresca y deportivas o de recreo (Anexo V del RD 1084/2009). </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wasteRM" type="wasteRMType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Recibo de residuos MARPOL (Anexo III del RD 1084/2009) </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wasteMAG" type="wasteMAGType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Memoria anual del gestor. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wasteST" type="wasteSTType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Estadísticas de residuos</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:schema>
