<xs:schema xmlns="e3l://eterproject.org/3.0/wasteSupport" xmlns:doc="e3l://eterproject.org/3.0/documentation" xmlns:common="e3l://eterproject.org/3.0/common" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xml="http://www.w3.org/XML/1998/namespace" targetNamespace="e3l://eterproject.org/3.0/wasteSupport" version="3.6">
	<xs:import namespace="e3l://eterproject.org/3.0/common" schemaLocation="e3l-common.xsd"/>
	<xs:annotation>
		<xs:appinfo>
			<doc:moduleDocs xml:lang="es">
				<doc:moduleName>e3l-wasteSupport</doc:moduleName>
				<doc:moduleDescription>
					<doc:section>
						<doc:title>Módulo e3l-wasteSupport - Diccionario de soporte del módulo waste</doc:title>
						<doc:para>El módulo de soporte e3l-wasteSupport contiene el diccionario de datos específico utilizado por el módulo waste.</doc:para>
					</doc:section>
				</doc:moduleDescription>
				<doc:recommendedUsage>Usado por el módulo waste y environmentalMasterData.</doc:recommendedUsage>
				<doc:standAlone>No</doc:standAlone>
			</doc:moduleDocs>
		</xs:appinfo>
	</xs:annotation>
	<!-- Autorización -->
	<xs:simpleType name="authorizationNumberType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Número de identificación de la autorización a una entidad / centro a ejercer cierta actividad relacionada con los tipos de autorización ambiental establecidos. Se incopora en ese elemento aquellas autorizaciones que responden al patrón acordado por las CCAA (Código CCAA + Código Tipo de Autorizacióm + 12 dígitos).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="17"/>
			<xs:pattern value="(0[1-9]|1[0-9])(P0[1-5]|G0[1-24-6]|[TANE]0[1-2]|SIR|SCR)\d{12}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="authorizationIdType">
		<xs:annotation>
			<xs:documentation xml:lang="es">En este elemento se identifican las inscripciones relacionadas con la gestión, el transporte y la producción de residuos.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="authorizationIdNumber" type="authorizationNumberType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Número de identificación de la inscripción a una entidad / centro a ejercer cierta actividad relacionada con los tipos de autorización ambiental establecidos. Se incopora en ese elemento aquellas autorizaciones que responden al patrón acordado por las CCAA (Código CCAA + Código Tipo de Autorizacióm + 12 dígitos).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="authorizationIdFree" type="common:notNullStringType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Número de inscripción Editable. Este elemento se introduce para aquellas CCAA que no hayan convertido sus inscripciones al patrón propuesto.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="authorizationIdEffects" type="authorizationEffectsType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Estado de la inscripción (En trámite o No disponible).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:simpleType name="authorizationCodeType">
