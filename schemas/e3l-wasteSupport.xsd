	<!-- Incidencias -->
	<xs:simpleType name="effectType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Esta codificación se utiliza en la identificación de incidencias de un traslado de residuo peligroso. Se incluye asimismo los aceites usados.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="01">
				<xs:annotation>
					<xs:documentation xml:lang="es">Modificación en los Kg Netos o brutos</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="02">
				<xs:annotation>
					<xs:documentation xml:lang="es">Modificación de los datos del transporte</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="03">
				<xs:annotation>
					<xs:documentation xml:lang="es">Inclusión de nuevos residuos en el DCS</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="99">
				<xs:annotation>
					<xs:documentation xml:lang="es">Otras</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="effectBlockType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Información de las incidencias del traslado.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="effect">
				<xs:annotation>
					<xs:documentation xml:lang="es">Identificación de la Incidencia</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="effectCode" type="effectType" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="es">Código de incidencia</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="effectDescription" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="es">Descripción incidencia</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="effectObservations" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="es">Observaciones de la incidencia</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- Almacenamiento -->
	<xs:simpleType name="storageCodeType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Este elemento identifica los posibles tipos de almacenamiento establecidos para residuos peligrosos en referencia a su exposición al medio.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="01">
				<xs:annotation>
					<xs:documentation xml:lang="es">Intemperie en el marco de esa insatalación</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="02">
				<xs:annotation>
					<xs:documentation xml:lang="es">Naves abiertas</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="03">
				<xs:annotation>
					<xs:documentation xml:lang="es">Naves cerradas</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="99">
				<xs:annotation>
					<xs:documentation xml:lang="es">Otros</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!-- Aceites Usados -->
	<xs:simpleType name="oilInstallationType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Identifica los tipos de instalaciones posibles para el destino de un traslado de Aceite Usado.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation xml:lang="es">Incineración</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation xml:lang="es">Recuperación (excepto recuperación energética)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="7">
				<xs:annotation>
					<xs:documentation xml:lang="es">Almacenamiento</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!-- Bolsa de residuos -->
	<xs:complexType name="bagResidueType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Este elemento permite la identificación por parte de gestores y productores de sus residuos</xs:documentation>
		</xs:annotation>
		<xs:attribute name="residueCode" type="residueIdType" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="es">Codificación del residuo en la bolsa de residuos (Introducir código 999 si es 'No disponible').</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="residueDescription" type="common:notNullStringType" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="es">Descripción del residuo con la que la entidad reconoce al residuo. Al contrario que la denominación L.E.R, esta descripción es libre y no sometida a formato alguno.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="residueIdType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Este elemento permite la identificación por parte de gestores y productores de sus residuos, de forma que con un simple código de 3 dígitos puede residir en combinación con el centro y el proceso de todos los datos técnicos del residuo que la Comunidad Autónoma guardará en su base de datos para su traducción.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="3"/>
			<xs:pattern value="\d{3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="residuesBagType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Elemento de la Bolsa de residuos del Productor/Gestor. La dupla nº de proceso, nº de residuo permite al productor/gestor identificar las diferentes 'bolsas de residuos' a gestionar para llevar a cabo su control, así como para realizar los correspondientes estudios de minimización que permitan establecer una mejora en sus procesos generadores desde el punto de vista ambiental.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="bagProcess" type="processType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Identificación del proceso generador o gestor del residuo.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bagResidueId" type="bagResidueType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Códificación del residuo en la bolsa de residuos (gestor o productor dependiendo de quien informa)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="residueConsistencyType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Estado del residuo</xs:documentation>
		</xs:annotation>
		<xs:attribute name="residueConsistencyCode" type="statisticConsistencyType" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="es">Código de estado del residuo</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="residueConsistencyDescription" type="xs:string">
			<xs:annotation>
				<xs:documentation xml:lang="es">Descripción de estado del residuo</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="processResidueBagType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Bolsa de residuos.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="residueLERType">
				<xs:sequence>
					<xs:element name="residueProcess" type="processType">
						<xs:annotation>
							<xs:documentation xml:lang="es">Identificación del proceso generador o gestor del residuo.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="residueId" type="bagResidueType">
						<xs:annotation>
							<xs:documentation xml:lang="es">Identificación interna que otroga la entidad al residuo del proceso en cuestión.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="residueOrigin">
						<xs:annotation>
							<xs:documentation xml:lang="es">Procedencia del residuo. Dependiendo del tipo de proceso que se trate (productor o gestor), existen cuatro posibilidades:	Producidos en Procesos Productores, Producidos en Procesos Gestores, Gestionados y Almacenamiento Intermedio.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:attribute name="originCode" type="residueOriginType" use="required">
								<xs:annotation>
									<xs:documentation xml:lang="es">Código de la procedencia del residuo.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="originDenomination" type="xs:string">
								<xs:annotation>
									<xs:documentation xml:lang="es">Descripción de la procedencia del residuo.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
					<xs:element name="residueTables" type="tablesDataMasterType">
						<xs:annotation>
							<xs:documentation xml:lang="es">Tablas de caracterización del residuo de acuerdo a la legislación vigente. Tablas Anexos del RD 833/1988 y 952/1997</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="residueQuantity">
						<xs:annotation>
							<xs:documentation xml:lang="es">Cantidad anual generada o gestionada de cada residuo peligroso en Kg</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:decimal">
								<xs:totalDigits value="15"/>
								<xs:fractionDigits value="3"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="residueStorage">
						<xs:annotation>
							<xs:documentation xml:lang="es">Almacenamiento del residuo.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="storageMonths">
									<xs:annotation>
										<xs:documentation xml:lang="es">Número de meses</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:integer">
											<xs:totalDigits value="2"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="storageContainer" type="containerDataType">
									<xs:annotation>
										<xs:documentation xml:lang="es">Tipo de recipiente donde se almacena</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="storageObservations" type="xs:string" minOccurs="0">
									<xs:annotation>
										<xs:documentation xml:lang="es">Observaciones acerca del almacenamiento</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="residueTechnicalData" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="es">Otros datos técnicos del residuo</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="residueFrequency" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="es">Frecuencia de retirada del residuo</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:attribute name="frequencyCode" type="retreatFrequencyType" use="required">
								<xs:annotation>
									<xs:documentation xml:lang="es">Código de la Frecuencia de retirada del residuo</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="frequencyDescription" type="xs:string">
								<xs:annotation>
									<xs:documentation xml:lang="es">Descripción de la Frecuencia de retirada del residuo</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
					<xs:element name="residueConsistency" type="residueConsistencyType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Estado del residuo</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="marpolCategory" type="marpolCategoryType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Categoria del residuo Marpol</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="statisticResiduesType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Categorías de residuos para la información estadística de residuos. La correspondencia está definida en la Lista B del Reglamento (CE) 782/2005.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:pattern value="([1-48] | T(N|H|T))"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="statisticActivityType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Categorías de actividades para la información estadística de residuos. La correspondencia con la lista NACE rev 1.1 (Reglamento (CE) nº 3037/90 del Consejo, última revisión Reglamento (CE) nº 1882/2003) está definida en la Lista C del Reglamento (CE) 782/2005.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:pattern value="([1-20]|TA)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="statisticConsistencyType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Clave que identifica Materia seca o residuos humedos en materia de estadísticas.(Ver Reglamento (CE) 782/2005)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="W">
				<xs:annotation>
					<xs:documentation xml:lang="es">Para todas las categorías de residuos, cantidad en residuos húmedos (normales)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D">
				<xs:annotation>
					<xs:documentation xml:lang="es">Para los lodos (números de residuo 11, 12, 40), también cantidad en materia seca.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="statisticUpdateType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Indica el carácter de los datos emitidos en materia estadística.(Ver Reglamento (CE) 782/2005)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="P">
				<xs:annotation>
					<xs:documentation xml:lang="es">Datos provisionales</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R">
				<xs:annotation>
					<xs:documentation xml:lang="es">Revisados</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="statisticConfidentialityType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Indica los datos confidenciales de acuerdo a artículo 4 apartado2 de la Directiva 2003/4/CE.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="a">
				<xs:annotation>
					<xs:documentation xml:lang="es">La confidencialidad de los procedimientos de las autoridades
públicas, cuando tal confidencialidad esté dispuesta por la
ley;</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="b">
				<xs:annotation>
					<xs:documentation xml:lang="es">las relaciones internacionales, la defensa nacional o la seguridad pública;</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="c">
				<xs:annotation>
					<xs:documentation xml:lang="es">la buena marcha de la justicia, la posibilidad de una persona de tener un juicio justo o la capacidad de una autoridad pública para realizar una investigación de índole penal o disciplinaria;</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="d">
				<xs:annotation>
					<xs:documentation xml:lang="es">la confidencialidad de datos de carácter comercial e industrial cuando dicha confidencialidad esté contemplada en la legislación nacional o comunitaria a fin de proteger intereses económicos legítimos, incluido el interés público de mantener la confidencialidad estadística y el secreto fiscal;</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="e">
				<xs:annotation>
					<xs:documentation xml:lang="es">los derechos de propiedad intelectual;</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="f">
				<xs:annotation>
					<xs:documentation xml:lang="es">el carácter confidencial de los datos y de los expedientes personales respecto de una persona física si esta persona no ha consentido en la revelación de esa información al público, cuando dicho carácter confidencial está previsto en el Derecho nacional o comunitario;</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="g">
				<xs:annotation>
					<xs:documentation xml:lang="es">los intereses o la protección de un tercero que haya facilitado voluntariamente la información solicitada sin estar obligado a ello por la ley o sin que la ley pueda obligarle a ello, salvo si esta persona ha consentido en su divulgación;</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="h">
				<xs:annotation>
					<xs:documentation xml:lang="es">la protección del medio ambiente al que se refiere la información, como por ejemplo la localización de especies raras.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="statisticCapacityVariableType">
		<xs:annotation>
			<xs:documentation xml:lang="es">La capacidad de instalaciones de recuperación y eliminación especificada en el conjunto 5 del Reglamento (CE) 782/2005 es posible medirlo en tres variables.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation xml:lang="es">Cantidad de instalaciones</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C">
				<xs:annotation>
					<xs:documentation xml:lang="es">Capacidad</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P">
				<xs:annotation>
					<xs:documentation xml:lang="es">Población afectada por el sistema de recogida</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="statisticCapacityMeasurementType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Especifica el código de medida de acuerdo a la variable de capacidad estudiada del Reglamento (CE) 782/2005.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="t">
				<xs:annotation>
					<xs:documentation xml:lang="es">Millares de toneladas al año con tres decimales</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="j">
				<xs:annotation>
					<xs:documentation xml:lang="es">Terajulios al año (10^12)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="m">
				<xs:annotation>
					<xs:documentation xml:lang="es">Metros cúbicos al año</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="statisticsSetType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Campos comunes necesarios para informar los distintos conjuntos que forman el sector Waste de estadísticas.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="STResidueLER">
				<xs:annotation>
					<xs:documentation xml:lang="es">Identificación del residuo de acuerdo a la Lista Europea de Residuos L.E.R. Anexo III del Reglamento 574/2004 de la Comisión Europea</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="LERCode" type="LERCodeType" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="es">Código L.E.R.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="LERDenomination" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="es">Denominación L.E.R. de acuerdo a la legislación europea en materia de residuos.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="STResidueNumber" type="statisticResiduesType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Número de residuo</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="STConsistencyCode" type="statisticConsistencyType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Código de consistencia</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="STResidueQuantity" type="common:decimalUnitsType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Cantidad de residuos en Kg</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="STResidueVolume" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="es">Volumen de residuos en m3</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="STUpdateIndicator" type="statisticUpdateType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Indicador de actualización</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="STConfidentialityIndicator" type="statisticConfidentialityType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Indicador de confidencialidad</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="statistics2SetType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Campos comunes necesarios para informar los distintos conjuntos que forman el sector Waste de estadísticas más la actividad de recuperación o eliminación.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="statisticsSetType">
				<xs:sequence>
					<xs:element name="STActivityNumber">
						<xs:annotation>
							<xs:documentation xml:lang="es">Actividad de recuperación
o eliminación</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="activityType">
								<xs:enumeration value="1"/>
								<xs:enumeration value="2"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="STOperation" type="table2Type">
						<xs:annotation>
							<xs:documentation xml:lang="es">Código de operación recuperación o eliminación</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- Residuos -->
	<xs:simpleType name="residueOriginType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Procedencia del residuo.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation xml:lang="es">Producidos en Procesos Productores: se muestran todos los residuos peligrosos producidos en procesos de producción (intervalo de proceso comprendido entre los códigos 0 y B08999 y los códigos B11000 a B11699 ambos inclusive. También los códigos B09711 y B99999 cuando el tipo de proceso es productor, este último ficticio.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation xml:lang="es">Producidos en Procesos Gestores: se muestran todos los residuos peligrosos producidos en procesos de gestión (intervalo de proceso comprendido entre los códigos B09000 inclusive a B11000 y mayores que el código B11699. También los códigos B09711 y B99999 cuando el tipo de proceso es gestor, este último ficticio.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation xml:lang="es">Gestionados: se muestran todos los residuos peligrosos gestionados (intervalo de proceso comprendido entre los códigos B09000 inclusive a B11000 y mayores que el código B11699. También los códigos B09711 y B99999 cuando el tipo de proceso es gestor, este último ficticio.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
