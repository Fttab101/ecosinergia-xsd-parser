					<xs:documentation xml:lang="es">Centro Gestor intermedio de residuos no peligrosos (almacenamiento)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="G06">
				<xs:annotation>
					<xs:documentation xml:lang="es">Plataforma logística</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="N01">
				<xs:annotation>
					<xs:documentation xml:lang="es">Negociante de residuos peligrosos</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="N02">
				<xs:annotation>
					<xs:documentation xml:lang="es">Negociante de residuos no peligrosos</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P01">
				<xs:annotation>
					<xs:documentation xml:lang="es">Productor de residuos peligrosos</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P02">
				<xs:annotation>
					<xs:documentation xml:lang="es">Pequeño productor de residuos peligrosos</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P03">
				<xs:annotation>
					<xs:documentation xml:lang="es">Productor de residuos no peligrosos</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P04">
				<xs:annotation>
					<xs:documentation xml:lang="es">Actividad productora de Residuos No Peligrosos en cantidad inferior a 1000 tn anuales y por tanto no sometida al régimen de comunicación previa.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P05">
				<xs:annotation>
					<xs:documentation xml:lang="es">Poseedor de residuos y, por tanto, no sometido a régimen de autorización o comunicación (accidentes, obras puntuales, comunidades de vecinos, ciudadanía,.....)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SCR">
				<xs:annotation>
					<xs:documentation xml:lang="es">Sistema colectivo de Responsabilidad ampliada</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SIR">
				<xs:annotation>
					<xs:documentation xml:lang="es">Sistema individual de Responsabilidad ampliada</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="T01">
				<xs:annotation>
					<xs:documentation xml:lang="es">Transportista de residuos peligrosos</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="T02">
				<xs:annotation>
