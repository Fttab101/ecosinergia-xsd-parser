											<xs:documentation xml:lang="es">Estimación de la cantidad de desechos que van a generarse entre la notifcación y el puerto de escala siguiente en m3</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="NMMovementSubstance" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="es">Especificar sustancia</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="NMMovementObservations" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="es">Observaciones generales</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:choice>
