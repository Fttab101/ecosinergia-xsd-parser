	<!-- Actividades -->
	<xs:simpleType name="activityType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Codificación estadística de los tipos de actividad gestora de residuos.Ver Lista F del Reglamento (CE) 782/2005. Se han incorporado dos tipos que incoporan los códigos que dicho reglamento no ha tenido en cuenta y que actúan por ello de 'cajón de sastre' : 6 (D8, D9,  D11 y D13) y 7 (R12, R13, D14 y D15).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation xml:lang="es">Incineración (recuperación energética)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation xml:lang="es">Incineración (eliminación)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation xml:lang="es">Recuperación (excepto recuperación energética)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation xml:lang="es">Eliminación grupo 1</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation xml:lang="es">Eliminación grupo 2</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation xml:lang="es">Eliminación grupo 3</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="7">
				<xs:annotation>
					<xs:documentation xml:lang="es">Almacenamiento</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
