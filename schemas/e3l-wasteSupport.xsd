	<!-- Datos Medioambientales de la entidad -->
	<xs:simpleType name="documentServiceType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Tipo de servicio del documento externo.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="externDocumentType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Datos sobre documentos externos que acompañan al fichero E3L.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="documentName" type="common:notNullStringType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Nombre del fichero</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentCode">
				<xs:annotation>
					<xs:documentation xml:lang="es">Código del tipo de documento externo. La utilización de este elemento es de carácter libre.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:long">
						<xs:totalDigits value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="documentDescription" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="es">Descripción libre del tipo de documento</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentService" type="documentServiceType">
				<xs:annotation>
					<xs:documentation xml:lang="es">Código del servicio / departamento de la CCAA que ha emitido del documento.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="dimensionType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Datos de dimensión del centro</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="usedPersonnel">
				<xs:annotation>
					<xs:documentation xml:lang="es">Total personal empleado</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="7"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="shiftsNumber">
				<xs:annotation>
					<xs:documentation xml:lang="es">Número de turnos 24 horas</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="operationDays">
				<xs:annotation>
					<xs:documentation xml:lang="es">Número de días de operación al año</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="installedPower">
				<xs:annotation>
					<xs:documentation xml:lang="es">Potencia instalada (KW)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="10"/>
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="transformersPower" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="es">Pontencia en transformadores (KVA)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="10"/>
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="contractedPower">
				<xs:annotation>
					<xs:documentation xml:lang="es">Potencia contratada (Kwh/año)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="16"/>
						<xs:fractionDigits value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="solarSurfaces" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="es">Superficie total solares (m2)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="buildingSurfaces" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="es">Superficie total edificaciones (m2)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="nominalCapacity" type="nominalCapacityType">
				<xs:annotation>
					<xs:documentation>Capacidad Nominal en Kg</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="nominalCapacityType">
		<xs:annotation>
			<xs:documentation>Capacidad Nominal</xs:documentation>
		</xs:annotation>
		<xs:attribute name="quantity" use="required">
			<xs:annotation>
				<xs:documentation>Cantidad que vendrá dada en Kg</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:decimal">
					<xs:totalDigits value="15"/>
					<xs:fractionDigits value="3"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="environmentalCenterWasteDataType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Datos del centro relacionados con la gestión o producción de residuos.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="centerProcesses" type="centerProcessType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="es">Procesos del centro, concepto que reconoceremos como bolsa de procesos.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="centerProcessesResidue" type="processResidueBagType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="es">Identificación de los residuos relacionados con el procesos del centro. Bolsa de residuos.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="centerAuthorization" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="es">En este elemento se identifican las inscripciones relacionadas con la gestión, el transporte y la producción de residuos a nivel de centro. Las  inscripciones a nivel de entidad deben ser identificadas en la etiqueta correpondiente.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="authorizationWasteType"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="centerDimension" type="dimensionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="es">Datos de dimensionamiento del centro.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="centerObservations" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="es">Observaciones generales que se desee incorporar respecto a temas ambientales y en concreto residuos.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="centerExternDocuments" type="externDocumentType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="es">Documentos externos que acompañan al fichero E3L.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="centerMarpol" type="common:marpolCenterType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="es">Información MARPOL relacionada con el centro</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="environmentalEntityWasteDataType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Datos de la entidad relacionados con la gestión o producción de resiudos y proporcionados en la memoria anual. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="entityManagingCentersNumber" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="es">Número de centros gestores de residuos en todo el estado.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="entityProductingCentersNumber" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="es">Número de centros productores de residuos en todo el estado.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="entityAuthorization" type="authorizationEntityWasteType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="es">En este elemento se identifican las inscripciones (autorizaciones y comunicaciones previas) relacionadas con la gestión, el transporte y la producción de residuos. Se informará de los códigos de tipo de inscripción que posee la entidad a nivel de entidad y de aquellos que se otorgan a nivel de centro. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="entityObservations" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="es">Observaciones generales que se desee incorporar respecto a temas ambientales y en concreto residuos.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="entityExternDocuments" type="externDocumentType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="es">Documentos externos que acompañan al fichero E3L.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="table2Type">
		<xs:annotation>
			<xs:documentation xml:lang="es">Código de la operación, una lista ampliada a partir de los anexos 1 y 2 de la Ley 22/2011, de 28 de julio, de residuos y suelos contaminados y los Anexos II y III de la nueva Ley 7/2022, de 8 de abril, de residuos y suelos contaminados para una economía circular. Se mantiene la denominación table2Type para facilitar la integración. Se mantienen las operaciones de tratamiento de los anexos 1 y 2 de la Ley 22/2011 que ya no aparecen en la Ley 7/2022, de 8 de abril. Se actualizan las descripciones de las operaciones que han cambiado en la Ley 7/2022, de 8 de abril. Se añaden las operaciones nuevas de la Ley 7/2022, de 8 de abril.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="D01">
				<xs:annotation>
					<xs:documentation xml:lang="es">Depósito sobre el suelo o en su interior (por ejemplo, vertido, etc.).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D02">
				<xs:annotation>
					<xs:documentation xml:lang="es">Tratamiento en medio terrestre (por ejemplo, biodegradación de residuos líquidos o lodos en el suelo, etc.).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D03">
				<xs:annotation>
					<xs:documentation xml:lang="es">Inyección en profundidad (por ejemplo, inyección de residuos bombeables en pozos, minas de sal o almacenes geológicos naturales, etc.).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D04">
				<xs:annotation>
					<xs:documentation xml:lang="es">Embalse superficial (por ejemplo, vertido de residuos líquidos o lodos en pozos, estanques o lagunas, etc.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D05">
				<xs:annotation>
					<xs:documentation xml:lang="es">Depósito controlado en lugares especialmente diseñados (por ejemplo, colocación en celdas estancas separadas, recubiertas y aisladas entre sí y del medio ambiente). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D06">
				<xs:annotation>
					<xs:documentation xml:lang="es">Vertido en medio acuático salvo en el mar </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D07">
				<xs:annotation>
					<xs:documentation xml:lang="es">Vertido en el mar, incluida la inserción en el lecho marino.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D08">
				<xs:annotation>
					<xs:documentation xml:lang="es">Tratamiento biológico no especificado en otros apartados del presente anexo que dé como resultado compuestos o mezclas que se eliminen mediante cualquiera de las operaciones numeradas D1 a D12.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D09">
				<xs:annotation>
					<xs:documentation xml:lang="es">Tratamiento físico-químico no especificado en otros apartados del presente anexo y que dé como resultado compuestos o mezclas que se eliminen mediante uno de los procedimientos numerados D1 a D12. </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D10">
				<xs:annotation>
					<xs:documentation xml:lang="es">Incineración en tierra</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D12">
				<xs:annotation>
					<xs:documentation xml:lang="es">Almacenamiento permanente (por ejemplo: colocación de contenedores en una mina, etc.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D13">
				<xs:annotation>
					<xs:documentation xml:lang="es">Combinación o mezcla previa a su eliminación mediante cualquiera de las operaciones numeradas D1 a D12</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D14">
				<xs:annotation>
					<xs:documentation xml:lang="es">Reenvasado previo a cualquiera de las operaciones numeradas D1 a D13</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D15">
				<xs:annotation>
					<xs:documentation xml:lang="es">Almacenamiento en espera de cualquiera de las operaciones numeradas D1 a D14 excluido el almacenamiento temporal en espera de recogida en el lugar en que se produjo el residuo</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R01">
				<xs:annotation>
					<xs:documentation xml:lang="es">Utilización principal como combustible u otro modo de producir energía </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R02">
				<xs:annotation>
					<xs:documentation xml:lang="es">Recuperación o Regeneración de disolventes </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R03">
				<xs:annotation>
					<xs:documentation xml:lang="es">Reciclado/recuperación de sustancias orgánicas que no se utilizan como disolventes (incluido el compostaje y otros procesos de transformación biológica)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R04">
				<xs:annotation>
					<xs:documentation xml:lang="es">Reciclado o recuperación de metales y de compuestos metálicos </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R05">
				<xs:annotation>
					<xs:documentation xml:lang="es">Reciclado o recuperación de otras materias inorgánicas </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R06">
				<xs:annotation>
					<xs:documentation xml:lang="es">Regeneración de ácidos o de bases</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R07">
				<xs:annotation>
					<xs:documentation xml:lang="es">Valorización componentes para reducir la contaminación</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R08">
				<xs:annotation>
					<xs:documentation xml:lang="es">Valorización de componentes procedentes de catalizadores</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R09">
				<xs:annotation>
					<xs:documentation xml:lang="es">Regeneración u otro nuevo empleo de aceites</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R10">
				<xs:annotation>
					<xs:documentation xml:lang="es">Tratamiento de suelos que produzca un beneficio a la agricultura o una mejora ecológica a los mismos</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R11">
				<xs:annotation>
					<xs:documentation xml:lang="es">Utilización de residuos obtenidos a partir de cualquiera de las operaciones numeradas de R1 a R10</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R12">
				<xs:annotation>
					<xs:documentation xml:lang="es">Intercambio de residuos para someterlos a cualquiera de las operaciones  enumeradas de R1 a R11. Quedan aquí incluidas operaciones previas a la valorización, incluido el tratamiento previo, previas a cualquiera de las operaciones  enumeradas de R1 a R11 </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R13">
				<xs:annotation>
					<xs:documentation xml:lang="es">Almacenamiento de residuos en espera de cualquiera de las operaciones numeradas de R1 a R12 (excluido el almacenamiento temporal, en espera de recogida, en el lugar donde se produjo el residuo)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R14">
				<xs:annotation>
					<xs:documentation xml:lang="es">Preparación para la reutilización</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R15">
				<xs:annotation>
					<xs:documentation xml:lang="es">Relleno</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="table3Type">
		<xs:annotation>
			<xs:documentation xml:lang="es">Esta tabla describe tipo genérico del residuo peligroso. Lista de valores de acuerdo a tabla 3 del anexo I del RD 952/97.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="L01">
				<xs:annotation>
					<xs:documentation xml:lang="es">Sustancias anatómicas: residuos hospitalarios u otros residuos clínicos.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L02">
				<xs:annotation>
					<xs:documentation xml:lang="es">Productos farmacéuticos, medicamentos, productos veterinarios.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L03">
				<xs:annotation>
					<xs:documentation xml:lang="es">Conservantes de la madera.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L04">
				<xs:annotation>
					<xs:documentation xml:lang="es">Biocidas y productos fitofarmacéuticos.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L05">
				<xs:annotation>
					<xs:documentation xml:lang="es">Residuos de productos utilizados como disolventes.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L06">
				<xs:annotation>
					<xs:documentation xml:lang="es">Sustancias orgánicas halogenadas no utilizadas como disolventes, excluidas las materias polimerizadas inertes.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L07">
				<xs:annotation>
					<xs:documentation xml:lang="es">Sales de temple cianuradas.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L08">
				<xs:annotation>
					<xs:documentation xml:lang="es">Aceites y sustancias oleosas minerales (lodos de corte, etcétera).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L09">
				<xs:annotation>
					<xs:documentation xml:lang="es">Mezclas aceite/agua o hidrocarburo/agua, emulsiones.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L10">
				<xs:annotation>
					<xs:documentation xml:lang="es">Sustancias que contengan PCB y/o PCT (dieléctricas, etcétera).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L11">
				<xs:annotation>
					<xs:documentation xml:lang="es">Materias alquitranadas procedentes de operaciones de refinado, destilación o pirólisis (sedimentos de destilación, etcétera).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L12">
				<xs:annotation>
					<xs:documentation xml:lang="es">Tintas, colorantes, pigmentos, pinturas, lacas, barnices.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L13">
				<xs:annotation>
					<xs:documentation xml:lang="es">Resinas, látex, plastificantes, colas.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L14">
				<xs:annotation>
					<xs:documentation xml:lang="es">Sustancias químicas no identificadas y/o nuevas y de efectos desconocidos en el
hombre y/o el medio ambiente que procedan de actividades de investigación y
desarrollo o de actividades de enseñanza (residuos de laboratorios, etcétera).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L15">
				<xs:annotation>
					<xs:documentation xml:lang="es">Productos pirotécnicos y otros materiales explosivos.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L16">
				<xs:annotation>
					<xs:documentation xml:lang="es">Sustancias químicas y productos de tratamiento utilizados en fotografía.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L17">
				<xs:annotation>
					<xs:documentation xml:lang="es">Todos los materiales contaminados por un producto de la familia de los dibenzofuranos
policlorados.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L18">
				<xs:annotation>
					<xs:documentation xml:lang="es">Todos los materiales contaminados por un producto de la familia de las
bienzo-para-dioxinas policloradas.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L19">
				<xs:annotation>
					<xs:documentation xml:lang="es">Jabones, materias grasas, ceras de origen animal o vegetal.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L20">
				<xs:annotation>
					<xs:documentation xml:lang="es">Sustancias orgánicas no halogenadas no empleadas como disolventes.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L21">
				<xs:annotation>
					<xs:documentation xml:lang="es">Sustancias inorgánicas que no contengan metales o compuestos de metales.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L22">
				<xs:annotation>
					<xs:documentation xml:lang="es">Escorias y/o cenizas.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L23">
				<xs:annotation>
					<xs:documentation xml:lang="es">Tierra, arcillas o arenas incluyendo lodos de dragado.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L24">
				<xs:annotation>
					<xs:documentation xml:lang="es">Sales de temple no cianuradas.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L25">
				<xs:annotation>
					<xs:documentation xml:lang="es">Partículas o polvos metálicos.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L26">
				<xs:annotation>
					<xs:documentation xml:lang="es">Catalizadores usados.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L27">
				<xs:annotation>
					<xs:documentation xml:lang="es">Líquidos o lodos que contengan metales o compuestos metálicos.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L28">
				<xs:annotation>
					<xs:documentation xml:lang="es">Residuos de tratamiento de descontaminación (polvos de cámaras de filtros de bolsas,
ectéceta), excepto los mencionados en los puntos 29, 30 y 33.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L29">
				<xs:annotation>
					<xs:documentation xml:lang="es">Lodos de lavado de gases.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L30">
				<xs:annotation>
					<xs:documentation xml:lang="es">Lodos de instalaciones de purificación de agua.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L31">
				<xs:annotation>
					<xs:documentation xml:lang="es">Residuos de descarbonatación.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L32">
				<xs:annotation>
					<xs:documentation xml:lang="es">Residuos de columnas intercambiadoras de iones.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L33">
				<xs:annotation>
					<xs:documentation xml:lang="es">Lodos de depuración no tratados o no utilizables en la agricultura.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L34">
				<xs:annotation>
					<xs:documentation xml:lang="es">Residuos de la limpieza de cisternas y/o equipos.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L35">
				<xs:annotation>
					<xs:documentation xml:lang="es">Equipos contaminados.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L36">
				<xs:annotation>
					<xs:documentation xml:lang="es">Recipientes contaminados (envases, bombonas de gas, etcétera) que hayan contenido
uno o varios de los constituyentes mencionados en la tabla 4.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L37">
				<xs:annotation>
					<xs:documentation xml:lang="es">Baterías y pilas eléctricas.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L38">
				<xs:annotation>
					<xs:documentation xml:lang="es">Aceites vegetales.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L39">
				<xs:annotation>
					<xs:documentation xml:lang="es">Objetos procedentes de recogidas selectivas de basuras domésticas y que presenten cualesquiera de las características mencionadas en la tabla 5.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L40">
				<xs:annotation>
					<xs:documentation xml:lang="es">Cualquier otro residuo que contenga uno cualesquiera de los constituyentes enumerados en la tabla 4 y presente cualesquiera de las características que se enuncian en la tabla 5.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P01">
				<xs:annotation>
					<xs:documentation xml:lang="es">Sustancias anatómicas: residuos hospitalarios u otros residuos clínicos.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P02">
				<xs:annotation>
					<xs:documentation xml:lang="es">Productos farmacéuticos, medicamentos, productos veterinarios.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P03">
				<xs:annotation>
					<xs:documentation xml:lang="es">Conservantes de la madera.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P04">
				<xs:annotation>
					<xs:documentation xml:lang="es">Biocidas y productos fitofarmacéuticos.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P05">
				<xs:annotation>
					<xs:documentation xml:lang="es">Residuos de productos utilizados como disolventes.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
