					<xs:documentation xml:lang="es">Centro Gestor intermedio de residuos peligrosos (almacenamiento)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="G04">
				<xs:annotation>
					<xs:documentation xml:lang="es">Centro Gestor de residuos no peligrosos</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="G05">
