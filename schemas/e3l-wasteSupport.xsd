	<xs:complexType name="authorizedProductsType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Tipo básico con la relación de productos autorizados para un sistema de responsabilidad ampliada.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="product" type="common:CPAType">
				<xs:annotation>
					<xs:documentation>Producto</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="productQuantity" type="nominalCapacityType">
				<xs:annotation>
					<xs:documentation>Cantidad anual estimada de producto puesto en el mercado. En kilogramos por producto.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="systemAdministrativeDataType">
		<xs:annotation>
			<xs:documentation>Información de la entidad administradora y de los tipos de integrantes del sistema. Información obligatoria para los sistemas colectivos de responsabilidad ampliada</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="common:entityType">
				<xs:sequence>
					<xs:element name="systemMembers" type="systemMembersType" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Integrantes del sistema colectivo (productores, distribuidores...)</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="extendedResponsabilitySystemType">
		<xs:annotation>
			<xs:documentation>Identificación de lo sistemas individuales y colectivos de Responsabilidad ampliada del productor del producto según lo descrito en la Ley 22/2011, de 28 de julio, </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="authorizedSystemProducts" type="authorizedProductsType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Productos autorizados</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice minOccurs="0">
				<xs:element name="systemProducer" type="productProducerType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Tipo de productor (Fabricante, Importador). Información obligatoria para los sistemas individuales de responsabilidad ampliada</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="systemAdministrativeData" type="systemAdministrativeDataType">
					<xs:annotation>
						<xs:documentation>Información obligatoria para las autorizaciones de sistemas colectivos de responsabilidad ampliada. (Identificación de la entidad administradora, datos de contacto de la entidad administradora e integrantes)</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="scope" type="common:CAType" nillable="true" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Ámbito de actuación. Se indican todas las CCAA donde actúa o bien el código 99 si su ambito es todo el país</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="residueOriginOrDestinationType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Datos de procedencia o destino del residuo. Tipo ambivalente. Para los residuos gestionados-entradas(etiqueta MAGProcessInputResidues) se identificarán los centros productores (procedencias) y para los residuos enviados a otro gestor-salidas (etiqueta MAGProcessOutputResidues) los centros gestores (destino final)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="entity" type="common:basicEntityType"/>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element name="center" type="common:centerIdType">
					<xs:annotation>
						<xs:documentation xml:lang="es">Identificación por centro origen o destino según corresponda.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="quantity" type="common:decimalUnitsType">
					<xs:annotation>
						<xs:documentation xml:lang="es">Cantidad en Kg</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Volume" type="common:decimalUnitsType" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="es">Volumen total de residuos enviados en m3</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="centerCNAE" type="common:CNAEType">
					<xs:annotation>
						<xs:documentation xml:lang="es">Código Nacional de Actividades Económicas según clasificación oficial CNAE-2009, RD 475/2007</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="registrationNumber" type="authorizationIdType">
					<xs:annotation>
						<xs:documentation xml:lang="es">Código de inscripción ambiental</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="registrationCode" type="authorizationCodeType"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="residueOriginManagementType">
		<xs:annotation>
			<xs:documentation xml:lang="es">Datos básicos de la entidad y tantos centros como sea necesario con las cantidades y los DCS generados.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="common:basicEntityType">
				<xs:sequence>
					<xs:choice>
						<xs:element name="centerDCSOrigin" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="es">Identificación por  DCS origen. Utilización para Memorias de residuos peligrosos y Memorias de residuos no peligroos con identificaciónn de NIMAS y DCS.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="common:spanishCenterIdType">
										<xs:sequence>
											<xs:element name="DCSQuantity" maxOccurs="unbounded">
												<xs:complexType>
													<xs:sequence>
														<xs:element name="DCS" type="common:notNullStringType">
															<xs:annotation>
																<xs:documentation xml:lang="es">Nº de DCS Nacional o transfronterizo.</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="quantity" type="common:decimalUnitsType">
															<xs:annotation>
																<xs:documentation xml:lang="es">Cantidad en Kg</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="centerOrigin" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="es">Identificación por centro origen Utilización para Memorias de residuos no peligrosos con identificación de NIMAS.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="common:spanishCenterIdType">
										<xs:sequence>
											<xs:element name="quantity" type="common:decimalUnitsType">
												<xs:annotation>
													<xs:documentation xml:lang="es">Cantidad en Kg</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="municipalyOrigin" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="es">Identificación por municipio origen. Utilización para Memorias de Residuos no pe sin identificación de NIMAS</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="common:municipalityDataType">
										<xs:sequence>
											<xs:element name="quantity" type="common:decimalUnitsType">
												<xs:annotation>
													<xs:documentation xml:lang="es">Cantidad en Kg</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="residueDestinationManagementType">
